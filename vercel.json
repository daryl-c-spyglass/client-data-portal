{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "dist/index.js": {
      "runtime": "@vercel/node@3",
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/dist/index.js" },
    { "source": "/auth/(.*)", "destination": "/dist/index.js" },
    { "source": "/health", "destination": "/dist/index.js" },
    { "source": "/(.*)", "destination": "/dist/public/$1" }
  ]
}
