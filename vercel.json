{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/auth/(.*)", "destination": "/auth/$1" },
    { "source": "/health", "destination": "/health" },
    { "source": "/(.*)", "destination": "/public/$1" }
  ],
  "functions": {
    "dist/index.js": {
      "runtime": "@vercel/node@3"
    }
  }
}
